  "type": "commonjs",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "get-version": "echo $npm_package_version",
    "lint": "npx eslint \"**/*.ts\" --ignore-pattern node_modules/ --fix",
