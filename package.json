    "typescript-eslint": "8.35.1"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.22.0",
    "axios": "1.10.0",
    "cors": "2.8.5",
    "dotenv": "16.6.1",
    "express": "5.1.0",
    "knex": "3.1.0",
    "pg": "8.16.3",
