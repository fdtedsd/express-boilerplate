    "build": "tsc && tsc-alias",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.2.3",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
