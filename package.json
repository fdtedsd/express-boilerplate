    "@types/node": "24.3.0",
    "eslint": "9.33.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.25.1",
    "axios": "1.12.0",
    "compression": "1.8.1",
    "cors": "2.8.5",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "knex": "3.1.0",
    "pg": "8.16.3",
    "uuid": "11.1.0",
    "zod": "4.0.17"
